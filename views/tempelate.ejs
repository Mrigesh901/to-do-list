
<%- include("header") -%>
    <div class="box" id ="heading">
     <h1>
     <%= listTitle %>
    </h1>
    </div>

    <div class="box">
        <% for(var i=0;i<itemlist.length;i++){ %> 
            <div class="item">
                <input type="checkbox">
                <p>
                  <%= itemlist[i] %> 
                </p>
            </div>
        <% } %>
       
        <form class="item" action="/" method="post">
            <input type="text" name = 'additem' placeholder="additem">
            <button type="submit" name="listtype" value="<%= listTitle%>">+</button>
        </form> 
    </div>


<%- include("footer"); -%>